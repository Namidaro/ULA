<UserControl x:Class="ULA.Devices.PICONGS.Presentation.Views.PICONGSConfigurationModeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:userControl1="clr-namespace:ULA.Presentation.Views.UserControl;assembly=ULA.Presentation"
             xmlns:userControls="clr-namespace:ULA.Devices.PICONGS.Presentation.Views.UserControls"
             xmlns:userControl="clr-namespace:ULA.Presentation.Infrastructure.ViewModels.UserControl;assembly=ULA.Presentation.Infrastructure"
             d:DesignHeight="800"
             d:DesignWidth="1000"
             mc:Ignorable="d"
                 Background="{StaticResource MainBackground}"
>
    <UserControl.Resources>
        <ControlTemplate x:Key="GroupBoxTemplate" TargetType="GroupBox">
            <Grid Margin="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <Border Grid.Row="0"
                        Background="#25A0DA"
                        BorderBrush="#25A0DA"
                        BorderThickness="1">
                    <Label Foreground="White">
                        <ContentPresenter Margin="4"
                                          ContentSource="Header"
                                          RecognizesAccessKey="True" />
                    </Label>
                </Border>

                <Border Grid.Row="1"
                        BorderBrush="#25A0DA"
                        BorderThickness="1,0,1,1">
                    <ContentPresenter Margin="4" />
                </Border>
            </Grid>
        </ControlTemplate>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="3*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <!--  указатель назад и Имя устройства  -->
        <StackPanel Grid.Row="0"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Orientation="Horizontal">

            <!--  Кнопка назад  -->
            <Viewbox Stretch="Fill">
                <Button Command="{Binding BackToSchemeCommand}"
                        SnapsToDevicePixels="True"
                        Style="{DynamicResource arrowBack}">
                    <Button.Resources>
                        <Style x:Key="arrowBack" TargetType="{x:Type Button}">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type Button}">
                                        <Grid>
                                            <Viewbox>
                                                <Grid>
                                                    <Path x:Name="arrowTop"
                                                          Width="262"
                                                          Height="198"
                                                          Data="F1 M 48.5143,170L 144.514,73.9999L 195.514,74L 121.515,150L 304.514,150L 304.514,190L 121.514,190L 195.514,266L 144.514,266L 48.5143,170 Z "
                                                          Fill="#FFF"
                                                          RenderTransformOrigin="0.5,0.5"
                                                          Stretch="Fill"
                                                          Stroke="#FFFF"
                                                          StrokeEndLineCap="Round"
                                                          StrokeLineJoin="Round"
                                                          StrokeStartLineCap="Round"
                                                          StrokeThickness="6">
                                                        <Path.RenderTransform>
                                                            <TransformGroup>
                                                                <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                                                <SkewTransform />
                                                                <RotateTransform />
                                                                <TranslateTransform />
                                                            </TransformGroup>
                                                        </Path.RenderTransform>
                                                    </Path>
                                                    <Ellipse x:Name="ellipse"
                                                             Width="340"
                                                             Height="340"
                                                             Fill="{DynamicResource Accent}"
                                                             Opacity=".6"
                                                             Stretch="Fill"
                                                             Stroke="#FFFFFF"
                                                             StrokeLineJoin="Round"
                                                             StrokeThickness="15" />
                                                    <Path x:Name="arrow"
                                                          Width="262"
                                                          Height="198"
                                                          Data="F1 M 48.5143,170L 144.514,73.9999L 195.514,74L 121.515,150L 304.514,150L 304.514,190L 121.514,190L 195.514,266L 144.514,266L 48.5143,170 Z "
                                                          Fill="White"
                                                          RenderTransformOrigin="0.5,0.5"
                                                          Stretch="Fill"
                                                          Stroke="#FFFF"
                                                          StrokeEndLineCap="Round"
                                                          StrokeLineJoin="Round"
                                                          StrokeStartLineCap="Round"
                                                          StrokeThickness="6">
                                                        <Path.RenderTransform>
                                                            <TransformGroup>
                                                                <ScaleTransform ScaleX="0.7" ScaleY="0.7" />
                                                                <SkewTransform />
                                                                <RotateTransform />
                                                                <TranslateTransform />
                                                            </TransformGroup>
                                                        </Path.RenderTransform>
                                                    </Path>

                                                </Grid>
                                            </Viewbox>
                                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                              RecognizesAccessKey="True"
                                                              SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                        </Grid>
                                        <ControlTemplate.Triggers>
                                            <Trigger Property="IsFocused" Value="True" />
                                            <Trigger Property="IsDefaulted" Value="True" />
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter TargetName="ellipse" Property="Opacity" Value="1" />
                                            </Trigger>
                                            <Trigger Property="IsPressed" Value="True">
                                                <Setter TargetName="ellipse" Property="Effect">
                                                    <Setter.Value>
                                                        <DropShadowEffect BlurRadius="50"
                                                                          Opacity=".75"
                                                                          RenderingBias="Performance"
                                                                          ShadowDepth="0"
                                                                          Color="White" />
                                                    </Setter.Value>
                                                </Setter>
                                                <Setter TargetName="arrow" Property="Effect">
                                                    <Setter.Value>
                                                        <BlurEffect Radius="30" RenderingBias="Performance" />
                                                    </Setter.Value>
                                                </Setter>
                                            </Trigger>
                                            <Trigger Property="IsEnabled" Value="False" />
                                        </ControlTemplate.Triggers>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </Button.Resources>
                </Button>
            </Viewbox>

            <StackPanel VerticalAlignment="Center" Orientation="Horizontal">
                <TextBlock VerticalAlignment="Center"
                           Style="{DynamicResource SubTitleStyle}"
                           Text="Конфигурация &quot;" />
                <TextBlock VerticalAlignment="Center"
                           Style="{DynamicResource SubTitleStyle}"
                           Text="{Binding DeviceName}" />
                <TextBlock VerticalAlignment="Center"
                           Style="{DynamicResource SubTitleStyle}"
                           Text="&quot;" />
            </StackPanel>
        </StackPanel>

        <Grid Grid.Row="1" Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <GroupBox Grid.Row="0"
                      Margin="3"
                      HorizontalAlignment="Stretch"
                      VerticalAlignment="Stretch"
                      Header="Настройка каналов"
                      Template="{StaticResource GroupBoxTemplate}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="№" />
                    <TextBlock Grid.Row="0"
                               Grid.Column="1"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="Данные КУ" />
                    <TextBlock Grid.Row="0"
                               Grid.Column="2"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="Выходы КУ" />
                    <TextBlock Grid.Row="0"
                               Grid.Column="3"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="Контроль управления" />
                    <TextBlock Grid.Row="1"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="1" />
                    <TextBlock Grid.Row="2"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="2" />
                    <TextBlock Grid.Row="3"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="3" />
                    <TextBlock Grid.Row="4"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="4" />
                    <TextBlock Grid.Row="5"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="5" />
                    <TextBlock Grid.Row="6"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="6" />
                    <TextBlock Grid.Row="7"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="7" />
                    <TextBlock Grid.Row="8"
                               Grid.Column="0"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               Text="8" />

                    <ComboBox Grid.Row="1"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[0],
                                                     Mode=TwoWay}" />
                    <ComboBox Grid.Row="2"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[1],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="3"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[2],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="4"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[3],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="5"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[4],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="6"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[5],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="7"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[6],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="8"
                              Grid.Column="1"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding DataKuCollection}"
                              SelectedItem="{Binding DataKuSelected[7],
                                                     Mode=TwoWay}" />


                    <ComboBox Grid.Row="1"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectionChanged="Selector_OnSelectionChanged"
                              SelectedItem="{Binding OutputsKuSelected[0],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="2"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              SelectionChanged="Selector_OnSelectionChanged"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectedItem="{Binding OutputsKuSelected[1],
                                                     Mode=TwoWay}" />
                    <ComboBox Grid.Row="3"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              SelectionChanged="Selector_OnSelectionChanged"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectedItem="{Binding OutputsKuSelected[2],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="4"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              SelectionChanged="Selector_OnSelectionChanged"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectedItem="{Binding OutputsKuSelected[3],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="5"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              SelectionChanged="Selector_OnSelectionChanged"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectedItem="{Binding OutputsKuSelected[4],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="6"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectionChanged="Selector_OnSelectionChanged"
                              SelectedItem="{Binding OutputsKuSelected[5],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="7"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              SelectionChanged="Selector_OnSelectionChanged"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectedItem="{Binding OutputsKuSelected[6],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="8"
                              Grid.Column="2"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                              SelectionChanged="Selector_OnSelectionChanged"
                              ItemsSource="{Binding OutputsKuCollection}"
                              SelectedItem="{Binding OutputsKuSelected[7],
                                                     Mode=TwoWay}" />


                    <ComboBox Grid.Row="1"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[0],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="2"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[1],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="3"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[2],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="4"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[3],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="5"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[4],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="6"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[5],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="7"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[6],
                                                     Mode=TwoWay}" />

                    <ComboBox Grid.Row="8"
                              Grid.Column="3"
                              Margin="5"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Center"
                               SelectionChanged="_cbManagemnt_SelectionChanged"
                              ItemsSource="{Binding ManagementKuCollection}"
                              SelectedItem="{Binding ManagementKuSelected[7],
                                                     Mode=TwoWay}" />
                </Grid>
            </GroupBox>
            <UniformGrid Grid.Row="1"
                         Margin="2"
                         HorizontalAlignment="Right"
                         VerticalAlignment="Top"
                         Columns="4"
                         Rows="1">
                <Button Width="Auto"
                        Margin="2"
                        HorizontalAlignment="Stretch"
                        Command="{Binding GetConfigurationFromFileCommand}"
                        Content="Прочитать из файла" />
                <Button Width="Auto"
                        Margin="2"
                        HorizontalAlignment="Stretch"
                        Command="{Binding SaveConfigurationInFileCommand}"
                        Content="Сохранить в файл"/>
                <Button Width="Auto"
                        Margin="2"
                        HorizontalAlignment="Stretch"
                        Command="{Binding GetConfiguration}"
                        Content="Прочитать" />
                <Button Width="Auto"
                        Margin="2"
                        HorizontalContentAlignment="Stretch"
                        Command="{Binding SendConfiguration}"
                        Content="Записать" />
            </UniformGrid>
        </Grid>

        <Grid Grid.Row="1" Grid.Column="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <TextBlock Text="{Binding RunoVersionDescription}"></TextBlock>
            <GroupBox Grid.Row="1"
                      Margin="3"
                      Header="Автоматика"
                      Template="{StaticResource GroupBoxTemplate}">
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="Время включения автоматики:" />
                    <TextBox Width="50"
                             Margin="5,0"
                             LostFocus="TimeToStartTextBox_OnLostFocus"
                             PreviewTextInput="TimeToStartTextBox_OnPreviewTextInput"
                             Text="{Binding TimeToStart,
                                            Mode=TwoWay}"
                             ToolTip="Значение должно быть в пределах от 0 до 65535" />
                    <TextBlock Text="c." />
                </StackPanel>
            </GroupBox>
            <GroupBox Grid.Row="2"
                      Margin="3"
                      Header="Маска неисправностей"
                      Template="{StaticResource GroupBoxTemplate}">
                <userControls:PICONGSConfigCheckBoxControlView HorizontalAlignment="Center"
                                                       VerticalAlignment="Center"
                                                       DataContext="{Binding FaultMask}"
                                                       PreviewMouseDown="FaultMask_OnPreviewMouseDown" />
            </GroupBox>
            <GroupBox Grid.Row="3"
                      Margin="3"
                      VerticalAlignment="Top"
                      Header="Неисправности"
                      Template="{StaticResource GroupBoxTemplate}">
                <Grid>
                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                        <StackPanel HorizontalAlignment="Stretch">
                            <ItemsControl ItemsSource="{Binding FaultCanals}" AlternationCount="8" >
                                <ItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel></StackPanel>
                                    </ItemsPanelTemplate>
                                </ItemsControl.ItemsPanel>
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate DataType="userControl:IConfigCheckBoxControlViewModel">
                                        <Expander Margin="5,2">
                                            <Expander.Header>
                                                <StackPanel Orientation="Horizontal">
                                                    <Ellipse Fill="Red" Height="5" Width="5" Margin="0,0,10,0" Visibility="{Binding Path=IsAtLeastOneSelected,Converter={StaticResource ObjectAsBoolToVisibilityConverter}}">
                                                    </Ellipse>
                                                    <StackPanel Orientation="Horizontal">
                                                        <TextBlock Text="Канал №"></TextBlock>
                                                        <TextBlock Text="{Binding Path=(ItemsControl.AlternationIndex),RelativeSource={RelativeSource TemplatedParent},Converter={StaticResource PlusOneConverter}}"></TextBlock>
                                                    </StackPanel>

                                                </StackPanel>
                                            </Expander.Header>
                                            <userControls:PICONGSConfigCheckBoxControlView HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center"
                                                                       DataContext="{Binding .,Mode=TwoWay}" />
                                        </Expander>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                            
                            
                            <Expander Margin="5,2" >
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <Ellipse Fill="Red" Height="5" Width="5" Margin="0,0,10,0" Visibility="{Binding FaultPower.IsAtLeastOneSelected,Converter={StaticResource ObjectAsBoolToVisibilityConverter}}">
                                        </Ellipse>
                                        <TextBlock Text="Питание"></TextBlock>
                                    </StackPanel>
                                </Expander.Header>
                                <userControls:PICONGSConfigCheckBoxControlView HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center"
                                                                       DataContext="{Binding FaultPower, Mode=TwoWay}" />
                            </Expander>
                            <Expander Margin="5,2" >
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <Ellipse Fill="Red" Height="5" Width="5" Margin="0,0,10,0" Visibility="{Binding FaultManagement.IsAtLeastOneSelected,Converter={StaticResource ObjectAsBoolToVisibilityConverter}}">
                                        </Ellipse>
                                        <TextBlock Text="Режим управления"></TextBlock>
                                    </StackPanel>
                                </Expander.Header>
                                <userControls:PICONGSConfigCheckBoxControlView HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center"
                                                                       DataContext="{Binding FaultManagement, Mode=TwoWay}" />
                            </Expander>
                            <Expander Margin="5,2">
                                <Expander.Header>
                                    <StackPanel Orientation="Horizontal">
                                        <Ellipse Fill="Red" Height="5" Width="5" Margin="0,0,10,0" Visibility="{Binding FaultSecurity.IsAtLeastOneSelected,Converter={StaticResource ObjectAsBoolToVisibilityConverter}}">
                                        </Ellipse>
                                        <TextBlock Text="Охрана"></TextBlock>
                                    </StackPanel>
                                </Expander.Header>
                                <userControls:PICONGSConfigCheckBoxControlView HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center"
                                                                       DataContext="{Binding FaultSecurity, Mode=TwoWay}" />
                            </Expander>
                        </StackPanel>
                    </ScrollViewer>
                </Grid>
            </GroupBox>
        </Grid>
    </Grid>
</UserControl>
