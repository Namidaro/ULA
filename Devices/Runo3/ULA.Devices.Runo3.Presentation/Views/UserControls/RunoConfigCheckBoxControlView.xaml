<UserControl x:Class="ULA.Devices.Runo3.Presentation.Views.UserControls.RunoConfigCheckBoxControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Row="0"
                   Grid.Column="1"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="1" />
        <TextBlock Grid.Row="0"
                   Grid.Column="2"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="2" />
        <TextBlock Grid.Row="0"
                   Grid.Column="3"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="3" />
        <TextBlock Grid.Row="0"
                   Grid.Column="4"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="4" />
        <TextBlock Grid.Row="0"
                   Grid.Column="5"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="5" />
        <TextBlock Grid.Row="0"
                   Grid.Column="6"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="6" />
        <TextBlock Grid.Row="0"
                   Grid.Column="7"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="7" />
        <TextBlock Grid.Row="0"
                   Grid.Column="8"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="8" />
        <TextBlock Grid.Row="0"
                   Grid.Column="9"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="9" />
        <TextBlock Grid.Row="0"
                   Grid.Column="10"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="10" />
        <TextBlock Grid.Row="0"
                   Grid.Column="11"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="11" />
        <TextBlock Grid.Row="1"
                   Grid.Column="0"
                   Margin="2"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   Text="1" />
        <CheckBox Grid.Row="1"
                  Grid.Column="1"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[0],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="2"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[1],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="3"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[2],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="4"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[3],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="5"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[4],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="6"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[5],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="7"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[6],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="8"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[7],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="9"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[8],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="10"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[9],
                                      Mode=TwoWay}" />
        <CheckBox Grid.Row="1"
                  Grid.Column="11"
                  Margin="2"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  IsChecked="{Binding CheckBoxResult[10],
                                      Mode=TwoWay}" />
    </Grid>
</UserControl>
